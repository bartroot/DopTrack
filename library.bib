isbn = {0949771414321394},
issn = {0949-7714},
journal = {Journal of Geodesy},
month = may,
number = {5},
pages = {259--268},
title = {{Regularization of geopotential determination from satellite data by variance components}},
url = {http://www.springerlink.com/openurl.asp?genre=article\&id=doi:10.1007/s00190-002-0245-x},
