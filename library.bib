keywords = {current-library-tag,igor \ae oem4-g2 \ae,polarx2 \ae,semicodeless tracking \ae,spaceborne gps \ae},
mendeley-tags = {current-library-tag},
month = mar,
number = {4},
pages = {249--261},
publisher = {Springer Berlin / Heidelberg},
title = {{Performance comparison of semicodeless GPS receivers for LEO satellites}},
