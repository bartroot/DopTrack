number = {4},
pages = {1--6},
title = {{A record-high ocean bottom pressure in the South Pacific observed by GRACE}},
url = {http://www.agu.org/pubs/crossref/2011/2010GL046013.shtml http://doi.wiley.com/10.1029/2010GL046013},
volume = {38},
