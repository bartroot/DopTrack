keywords = {analytical,annual,change,cycles,determination,diurnal,general,geodesy,geopotential,global,gravity,interactions,modeling,ocean-earth-atmosphere,oceanography,seasonal,theory,water},
mendeley-tags = {analytical,annual,change,cycles,determination,diurnal,general,geodesy,geopotential,global,gravity,interactions,modeling,ocean-earth-atmosphere,oceanography,seasonal,theory,water},
