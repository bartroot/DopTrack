keywords = {analysis,empirical,error,functions,geodesy,gps,grace,gravity,harmonics,interactions,ocean-earth-atmosphere-hydrosphere-cryosphere,orthogonal,results,satellite,spherical,temporary,time,variable},
mendeley-tags = {analysis,empirical,error,functions,geodesy,gps,grace,gravity,harmonics,interactions,ocean-earth-atmosphere-hydrosphere-cryosphere,orthogonal,results,satellite,spherical,temporary,time,variable},
month = aug,
