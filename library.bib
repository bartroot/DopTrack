journal = {Advances in Space Research},
keywords = {altimetry,doris,grace,mascon,pod,time varying gravity (tvg)},
month = apr,
number = {null},
title = {{DORIS-based point mascons for the long term stability of precise orbit solutions}},
url = {http://dx.doi.org/10.1016/j.asr.2013.03.023 http://linkinghub.elsevier.com/retrieve/pii/S0273117713001762},
