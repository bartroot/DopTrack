@inproceedings{Encarnacao2008,
abstract = {The major goal of future gravimetric satellite missions is to determine the time-varying gravity field so that processes of mass transport on the Earth are better understood. The purpose of the paper is to show the dependency of the accuracy of gravity field recovery on the type of formation. It also intends to show that careful selection of the formation is of utmost importance for the accuracy of the recovered gravity field. The formations considered are trailing, pendulum and cart-wheel. Additionally, the combined inversion considering the observations from the trailing and pendulum formations will be produced, thus simulating a formation in the shape of a 'T'. Several noise scenarios are simulated: computational errors (noiseless simulations), sensor noise and noise in the position. The results show that the larger the inter-satellite ranges, the more accurate is the determination of the gravity field. The simulations indicate that there is a lower sensitivity to gravitational features with similar wavelength as the inter-satellite distance of the trailing formation. The pendulum formation reveals the surprising insensitivity to uncertainties in the relative position, while the solutions from other formations are heavily corrupted by it. This report presents preliminary results and serves as a conceptual guideline for future research.},
author = {Encarna\c{c}\~{a}o, Jo\~{a}o and Ditmar, Pavel and Liu, Xianglin},
booktitle = {3rd International Symposium on Formation Flying, Missions and Technologies},
keywords = {current-library-tag},
